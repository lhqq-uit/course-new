                                                </div>
                                            </div>
                                            <div class="col text-right">
                                                <a href="/course/<%= item.id_course._id %>" class="btn btn-primary">Watch trailer</a>
                                            </div>
                                        </div>
        
                                    </div>
        
                            </div>
                            <% }) %>
                        <% } %>
                        
                    </div>

                    <div class="page-heading">
                        <h4>Achievements</h4>
                        <a href="#" class="text-underline ml-sm-auto">My achievements</a>
                    </div>

                    <div class="col-lg-12">
                        <div id="carouselExampleFade" class="carousel carousel-card slide mb-24pt">
                            <div class="carousel-inner">
                                <% if (getCourseStudied.length > 0) { %>
                                    <% getCourseStudied.forEach(function(item) { %>

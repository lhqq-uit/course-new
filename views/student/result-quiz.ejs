    <script>
            var result_answer = JSON.parse(localStorage.getItem("boolean_answer"));
            var your_answer = JSON.parse(localStorage.getItem("answer"));
            let your_score = 0;
            let total_score = result_answer.length * 10;
            for(let i=0; i<your_answer.length; i++) {
                if(result_answer[i] == true){
                    your_score += 10;
                    if(your_answer[i] == "A"){
                    document.querySelectorAll('.answer-A')[i].classList.add('list-quiz-badge-success');
                    } else if( your_answer[i] == "B"){
                        document.querySelectorAll('.answer-B')[i].classList.add('list-quiz-badge-success');
                    } else if( your_answer[i] == "C"){
                        document.querySelectorAll('.answer-C')[i].classList.add('list-quiz-badge-success');
                    } else{
                        document.querySelectorAll('.answer-D')[i].classList.add('list-quiz-badge-success');
                    }
                } else{
                    if(your_answer[i] == "A"){
                    document.querySelectorAll('.answer-A')[i].classList.add('list-quiz-badge-error');
                    } else if( your_answer[i] == "B"){
                        document.querySelectorAll('.answer-B')[i].classList.add('list-quiz-badge-error');
                    } else if( your_answer[i] == "C"){
                        document.querySelectorAll('.answer-C')[i].classList.add('list-quiz-badge-error');
                    } else{
                        document.querySelectorAll('.answer-D')[i].classList.add('list-quiz-badge-error');
                    }
                }
            }
            document.getElementById('your-score').innerHTML = "You score: " + your_score;
            document.getElementById('score').innerHTML = `${your_score}/${total_score} Score`
    </script>


